{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ep-dataintegration-prod01"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/db_TAS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "Sybase",
				"typeProperties": {
					"server": "epbpwe-tas01.corp.euroports.com",
					"database": "CTOPDATAEPEPDB",
					"authenticationType": "Basic",
					"username": "TAS",
					"password": {
						"type": "SecureString",
						"value": "**********"
					}
				},
				"connectVia": {
					"referenceName": "adfir-shruntime-prod01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": []
		}
	]
}